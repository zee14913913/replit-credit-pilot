        "(?:Payment Due Date|Due Date)\\s*[:\\s]+(\\d{1,2}[/-]\\d{1,2}[/-]\\d{2,4})",
        "(?:IMMEDIATE|Segera)"
      ],
      "previous_balance": [
        {
          "regex": [
            "PREVIOUS BALANCE[\\s\\S]{0,50}?([\\d,]+\\.\\d{2})"
          ]
        },
        "(?:Previous Balance|Balance B/?F|Baki Terdahulu|Opening Balance)\\s*[:\\s]+(?:RM\\s*|MYR\\s*)?([-]?[\\d,]+\\.\\d{2})",
        "(?:Baki Dibawa)\\s*[:\\s]+(?:RM\\s*)?([-]?[\\d,]+\\.\\d{2})",
        "(?:Previous|Last)\\s+(?:Balance|Baki)\\s*[:\\s]*(?:RM\\s*)?([-]?[\\d,]+\\.\\d{2})"
