        "(?:Min\\.?\\s*Pay|Bayaran Min)\\s*[:\\s]+(?:RM\\s*)([\\d,]+\\.\\d{2})",
        "(?:Please Pay At Least|Sila Bayar Sekurang)\\s*[:\\s]*(?:RM\\s*)([\\d,]+\\.\\d{2})"
