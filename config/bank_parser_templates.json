        "(?:Approved Limit|Had Diluluskan)\\s*[:\\s]+(?:RM\\s*)([\\d,]+\\.\\d{2})",
        "(?:Your Credit Limit)\\s*[:\\s]*(?:RM\\s*)([\\d,]+\\.\\d{2})"
