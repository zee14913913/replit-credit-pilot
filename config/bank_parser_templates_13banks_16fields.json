{
  "AMBANK": {
    "bank_name": "AMBANK",
    "aliases": ["AMBANK", "AmBank", "AMBANK ISLAMIC", "AmBank Islamic"],
    "patterns": {
      "customer_name": {
        "regex": [
          "MALAYSIA\\n([A-Z][A-Z\\s]{8,45})\\n",
          "^([A-Z]{5,}\\s+[A-Z]{3,}\\s+[A-Z]{3,})$",
          "NEGERI SEMBILAN\\n([A-Z\\s]{10,45})\\n"
        ]
      },
      "ic_no": {
        "regex": [
          "I/?C\\s*(?:No\\.?|Number)?[:\\s]*(\\d{6}-\\d{2}-\\d{4})",
          "NRIC[:\\s]*(\\d{6}-\\d{2}-\\d{4})",
          "(\\d{12})"
        ]
      },
      "card_type": {
        "regex": [
          "(AmBank Islamic Visa Signature)",
          "(Visa\\s+Signature)",
          "(BonusLink.*?Visa)",
          "(UnionPay)",
          "Card Type.*?\\n(.+?)\\n"
        ]
      },
      "card_no": {
        "regex": [
          "(\\d{4})\\s+(\\d{4})\\s+(\\d{4})\\s+(\\d{4})",
          "(\\d{4})-(\\d{4})-(\\d{4})-(\\d{4})"
        ]
      },
      "credit_limit": {
        "regex": [
          "Total Credit Limit[\\s\\S]{0,100}?([\\d,]+)",
          "Jumlah Had Kredit[\\s\\S]{0,100}?([\\d,]+)",
          "Credit Limit[\\s\\S]{0,100}?([\\d,]+)"
        ]
      },
      "statement_date": {
        "regex": [
          "Statement Date[\\s\\S]{0,100}?(\\d{1,2}\\s+[A-Z]{3}\\s+\\d{2,4})",
          "Tarikh Penyata[\\s\\S]{0,100}?(\\d{1,2}\\s+[A-Z]{3}\\s+\\d{2,4})"
        ]
      },
      "payment_due_date": {
        "regex": [
          "Payment Due Date[\\s\\S]{0,100}?(\\d{1,2}\\s+[A-Z]{3}\\s+\\d{2,4})",
          "Tarikh Matang Bayaran[\\s\\S]{0,100}?(\\d{1,2}\\s+[A-Z]{3}\\s+\\d{2,4})"
        ]
      },
      "full_due_amount": {
        "regex": [
          "Total Current Balance[\\s\\S]{0,50}?\\n([\\d,]+\\.\\d{2})",
          "Current Balance[\\s\\S]{0,100}?RM\\s*([\\d,]+\\.\\d{2})",
          "Baki Semasa[\\s\\S]{0,100}?([\\d,]+\\.\\d{2})"
        ]
      },
      "minimum_payment": {
        "regex": [
          "Minimum Payment[\\s\\S]{0,200}?RM\\s*([\\d,]+\\.\\d{2})",
          "Bayaran Minimum[\\s\\S]{0,100}?RM\\s*([\\d,]+\\.\\d{2})",
          "Minimum.*?\\n([\\d,]+\\.\\d{2})"
        ]
      },
      "previous_balance": {
        "regex": [
          "PREVIOUS BALANCE[\\s\\S]{0,500}?\\n([\\d,]+\\.\\d{2})",
          "Baki Terdahulu[\\s\\S]{0,100}?([\\d,]+\\.\\d{2})",
          "Previous Balance[\\s\\S]{0,100}?([\\d,]+\\.\\d{2})"
        ]
      },
      "transaction_date": {
        "regex": [
          "(\\d{2}\\s+[A-Z]{3})",
          "(\\d{2}/\\d{2}/\\d{2})",
          "(\\d{1,2}\\s+[A-Z][a-z]{2}\\s+\\d{2})"
        ]
      },
      "description": {
        "regex": [
          "\\d{2}\\s+[A-Z]{3}\\s+(.+?)(?=\\s+[\\d,]+\\.\\d{2}|$)",
          "Transaction Description[\\s\\n]+(.+?)(?=\\d|Amount)",
          "\\d{2}/\\d{2}\\s+(.+?)\\s+[\\d,]+\\.\\d{2}"
        ]
      },
      "amount_CR": {
        "regex": [
          "([\\d,]+\\.\\d{2})\\s+CR",
          "PAYMENT.*?([\\d,]+\\.\\d{2})",
          "REFUND.*?([\\d,]+\\.\\d{2})"
        ]
      },
      "amount_DR": {
        "regex": [
          "([\\d,]+\\.\\d{2})(?!\\s*CR)",
          "^(?!.*PAYMENT).*?([\\d,]+\\.\\d{2})"
        ]
      },
      "earned_point": {
        "regex": [
          "Points Earned[\\s\\S]{0,50}?([\\d,]+)",
          "AmBonus[\\s\\S]{0,200}?\\n([\\d,]+)\\n",
          "Total Points[\\s\\S]{0,100}?([\\d,]+)"
        ]
      }
    }
  },
  
  "AMBANK_ISLAMIC": {
    "bank_name": "AMBANK_ISLAMIC",
    "aliases": ["AmBank Islamic", "AMBANK ISLAMIC", "AmBank-i"],
    "patterns": {
      "customer_name": {
        "regex": [
          "MALAYSIA\\n([A-Z][A-Z\\s]{8,45})\\n",
          "^([A-Z]{5,}\\s+[A-Z]{3,}\\s+[A-Z]{3,})$"
        ]
      },
      "ic_no": {
        "regex": [
          "I/?C\\s*(?:No\\.?|Number)?[:\\s]*(\\d{6}-\\d{2}-\\d{4})",
          "NRIC[:\\s]*(\\d{6}-\\d{2}-\\d{4})"
        ]
      },
      "card_type": {
        "regex": [
          "(AmBank.*?Visa.*?)",
          "(UnionPay)",
          "(BonusLink)"
        ]
      },
      "card_no": {
        "regex": [
          "(\\d{4})\\s+(\\d{4})\\s+(\\d{4})\\s+(\\d{4})"
        ]
      },
      "credit_limit": {
        "regex": [
          "Total Credit Limit[\\s\\S]{0,100}?([\\d,]+)",
          "Had Kredit[\\s\\S]{0,100}?([\\d,]+)"
        ]
      },
      "statement_date": {
        "regex": [
          "Statement Date[\\s\\S]{0,100}?(\\d{1,2}\\s+[A-Z]{3}\\s+\\d{2,4})"
        ]
      },
      "payment_due_date": {
        "regex": [
          "Payment Due Date[\\s\\S]{0,100}?(\\d{1,2}\\s+[A-Z]{3}\\s+\\d{2,4})"
        ]
      },
      "full_due_amount": {
        "regex": [
          "Current Balance[\\s\\S]{0,100}?([\\d,]+\\.\\d{2})",
          "Baki Semasa.*?\\n([\\d,]+\\.\\d{2})"
        ]
      },
      "minimum_payment": {
        "regex": [
          "Minimum Payment.*?\\n([\\d,]+\\.\\d{2})",
          "Bayaran Minimum.*?\\n([\\d,]+\\.\\d{2})"
        ]
      },
      "previous_balance": {
        "regex": [
          "PREVIOUS BALANCE[\\s\\S]{0,500}?\\n([\\d,]+\\.\\d{2})"
        ]
      },
      "transaction_date": {
        "regex": [
          "(\\d{2}\\s+[A-Z]{3})"
        ]
      },
      "description": {
        "regex": [
          "\\d{2}\\s+[A-Z]{3}\\s+(.+?)(?=\\s+[\\d,]+\\.\\d{2})"
        ]
      },
      "amount_CR": {
        "regex": [
          "([\\d,]+\\.\\d{2})\\s+CR"
        ]
      },
      "amount_DR": {
        "regex": [
          "([\\d,]+\\.\\d{2})(?!\\s*CR)"
        ]
      },
      "earned_point": {
        "regex": [
          "AmBonus[\\s\\S]{0,200}?\\n([\\d,]+)"
        ]
      }
    }
  }
}
